<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
version="2.0"> 

    <managed-bean>
        <managed-bean-name>hello</managed-bean-name>
        <managed-bean-class>br.unisul.projeto.view.backbean.HelloBack</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>cadastroUsuario</managed-bean-name>
        <managed-bean-class>br.unisul.projeto.view.backbean.CadastroUsuarioBack</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <navigation-rule>
        <from-view-id>/hello.jsp</from-view-id>
        <navigation-case>
            <from-outcome>retorno1</from-outcome>
            <to-view-id>/hello1.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>retorno2</from-outcome>
            <to-view-id>/hello2.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    
</faces-config>









